{
    "name": "afaya/edget-tts",
    "description": "Microsoft Edge's TTS",
    "version": "1.0.0",
    "type": "project",
    "license": "GPL-3.0+",
    "repository": {
        "type": "git",
        "url": "https://github.com/andresayac/edge-tts"
    },
    "require": {
        "symfony/console": "^7.1",
        "ratchet/pawl": "^0.4.1",
        "ramsey/uuid": "^4.7",
        "react/event-loop": "^1.5"
    },
    "autoload": {
        "psr-4": {
             "Afaya\\EdgeTTS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Afaya\\EdgeTTS\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php main.php"
    },
    "bin": [
        "src/cli.php"
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}